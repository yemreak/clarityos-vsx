	"tokenColors": [
		{
			"scope": ["source"],
			"settings": {
				"foreground": "#c1c1c1"
			}
		},
		{
			"scope": ["variable"],
			"settings": {
				"foreground": "#c1c1c1",
				"fontStyle": "italic"
			}
		},
		{
			"name": "functions parameters",
			"scope": ["variable.parameter", "source variable.parameter", "meta.method.declaration", "source.pip-requirements", "meta.function-call.arguments", "meta.attribute", "meta.fstring"],
			"settings": {
				"foreground": "#b7b7b7",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["markup.list.numbered.markdown"],
			"settings": {
				"foreground": "#c6c6c6",
				"fontStyle": "italic"
			}
		},
		{
			"name": "functions",
			"scope": ["meta.definition.method", "meta.function.shell", "entity.name.function"],
			"settings": {
				"foreground": "#F1BE34"
			}
		},
		{
			"name": "functions calls",
			"scope": ["meta.function-call", "meta.statement.command.shell", "meta.function-call.generic", "support.class.promise"],
			"settings": {
				"foreground": "#f1be34",
				"fontStyle": "italic"
			}
		},
		{
			"name": "properties",
			"scope": ["variable.object.property", "variable.other.property", "variable.other.object.property", "meta.object-literal.key"],
			"settings": {
				"foreground": "#adadad"
			}
		},
		{
			"scope": ["support.variable.property", "meta.type.parameters support.type"],
			"settings": {
				"foreground": "#adadad",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "operators",
			"scope": ["source keyword.operator", "punctuation.separator", "punctuation.section"],
			"settings": {
				"foreground": "#A3C3C7"
			}
		},
		{
			"name": "decorators, @property, __init__",
			"scope": ["punctuation.definition.decorator", "support.function.builtin", "support.function.magic", "support.type.python", "support.variable.magic", "entity.name.function.decorator.python", "keyword", "string.regexp"],
			"settings": {
				"foreground": "#A3C3C7",
				"fontStyle": "italic"
			}
		},
		{
			"name": "none, null, undefined, imports, throw, exception",
			"scope": ["punctuation.accessor", "keyword.operator.logical", "meta.template.expression punctuation.definition", "keyword.operator.new", "entity.name.section", "entity.name.tag", "constant.language.python", "support.type.property-name.json"],
			"settings": {
				"foreground": "#cb8800"
			}
		},
		{
			"name": "this, super, self",
			"scope": ["variable.language", "support.type.property-name", "support.type.exception", "variable.other.readwrite.alias"],
			"settings": {
				"foreground": "#cb8800",
				"fontStyle": "italic"
			}
		},
		{
			"name": "throw, exception",
			"scope": ["keyword.control.trycatch"],
			"settings": {
				"foreground": "#cf7058",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["punctuation.definition.template-expression.begin", "punctuation.definition.template-expression.end"],
			"settings": {
				"foreground": "#cf705881",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language"],
			"settings": {
				"foreground": "#D19A66"
			}
		},
		{
			"name": "primitive types",
			"scope": ["meta.type.parameters", "support.type.primitive.ts", "support.type.builtin", "entity.name.function.decorator", "support.type"],
			"settings": {
				"foreground": "#E4BEDF",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#C996A7",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.type.alias",
			"settings": {
				"foreground": "#C996A7"
			}
		},
		{
			"name": "class",
			"scope": ["entity.name.type.class", "variable.other.constant.object"],
			"settings": {
				"foreground": "#eb9744"
			}
		},
		{
			"name": "inhertited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#eb9744",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["storage.type", "keyword", "keyword.control", "storage.modifier", "storage.type.property", "keyword.operator.logical.python", "keyword.operator.expression"],
			"settings": {
				"foreground": "#B180D7"
			}
		},
		{
			"scope": ["string", "string.unquoted.plain.out.yaml"],
			"settings": {
				"foreground": "#98C300"
			}
		},
		{
			"scope": "string.template",
			"settings": {
				"foreground": "#98C300",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["markup.underline.link.clarityos.file", "string.unquoted.link.clarityos.file"],
			"settings": {
				"foreground": "#de9c0d",
				"fontStyle": "underline"
			}
		},
		{
			"scope": ["punctuation.definition.heading.markdown", "punctuation.definition.markdown", "fenced_code.block.language.markdown"],
			"settings": {
				"foreground": "#6f6f6d"
			}
		},
		{
			"scope": ["markup.inline.raw.string.markdown", "markup.inline.raw"],
			"settings": {
				"foreground": "#CF7058"
			}
		},
		{
			"scope": "log.warning",
			"settings": {
				"foreground": "#F1BE34"
			}
		},
		{
			"scope": "log.error",
			"settings": {
				"foreground": "#d77158"
			}
		},
		{
			"scope": "log.info",
			"settings": {
				"foreground": "#698ac9"
			}
		},
		{
			"scope": "log.debug",
			"settings": {
				"foreground": "#6b7992"
			}
		},
		{
			"scope": "string.quoted.docstring.multi",
			"settings": {
				"foreground": "#6a9500"
			}
		},
		{
			"scope": ["comment", "markup.quote", "markup.raw.block.markdown"],
			"settings": {
				"foreground": "#6b7992",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "log.warning",
			"settings": {
				"foreground": "#F1BE34",
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": "log.error",
			"settings": {
				"foreground": "#d77158",
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": ["log.debug", "keyword.codetag.notation"],
			"settings": {
				"foreground": "#6b7992",
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": ["log.info"],
			"settings": {
				"foreground": "#698ac9",
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": ["variable.other.constant", "constant"],
			"settings": {
				"foreground": "#698ac9",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["variable.other.enummember"],
			"settings": {
				"foreground": "#698ac9"
			}
		},
		{
			"name": "Hidden items",
			"scope": "markup.underline.link",
			"settings": {
				"foreground": "#555555"
			}
		},
		{
			"name": "Swift storage modifiers",
			"scope": ["storage.modifier.attribute.swift"],
			"settings": {
				"foreground": "#F1BE34",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Swift type body definition",
			"scope": ["meta.definition.type.body.swift"],
			"settings": {
				"foreground": "#c1c1c1"
			}
		}
	]
}
